  "properties": {
    "reopenedDate": {
      "type": "string",
      "format": "date"
    },
    "libraCaseNumber": {
      "type": "string",
      "minLength": 1
    },
    "reason": {
      "type": "string",
      "minLength": 1
    }
  },
