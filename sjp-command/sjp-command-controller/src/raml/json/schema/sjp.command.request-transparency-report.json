{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/command/sjp.command.request-transparency-report.json",
  "type": "object",
  "properties": {
    "format": {
      "type": "string",
      "enum": [
        "PDF",
        "JSON"
      ]
    },
    "requestType": {
      "type": "string",
      "enum": [
        "FULL",
        "DELTA"
      ]
    },
    "language": {
      "type": "string",
      "enum": [
        "ENGLISH",
        "WELSH"
      ]
    }
  },
  "required": [
    "format", "language"
  ]
}