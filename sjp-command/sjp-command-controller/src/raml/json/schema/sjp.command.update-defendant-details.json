{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "string"
    },
    "gender": {
      "enum": [
        "Male",
        "Female",
        "Not Specified"
      ]
    },
    "email": {
      "type": "string"
    },
    "address": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/address.json"
    },
    "contactNumber": {
      "type": "object",
      "properties": {
        "home": {
          "type": "string"
        },
        "mobile": {
          "type": "string"
        }
      }
    },
    "nationalInsuranceNumber": {
      "type": "string"
    }
  }
}
