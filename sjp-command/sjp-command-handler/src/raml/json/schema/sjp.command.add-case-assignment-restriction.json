{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/sjp.command.add-case-assignment-restriction.json",
  "type": "object",
  "properties": {
    "prosecutingAuthority": {
      "type": "string"
    },
    "includeOnly": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "exclude": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "validFrom": {
      "$ref": "#/definitions/isoDate"
    },
    "validTo": {
      "$ref": "#/definitions/isoDate"
    }
  },
  "required": [
    "prosecutingAuthority",
    "includeOnly",
    "exclude"
  ],
  "definitions": {
    "isoDate": {
      "type": "string",
      "pattern": "^\\d{4}-[01]\\d-[0-3]\\d$"
    }
  },
  "additionalProperties": false
}