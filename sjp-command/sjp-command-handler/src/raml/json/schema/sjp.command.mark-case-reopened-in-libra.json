{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/command/sjp.mark-case-reopened-in-libra.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    },
    "reopenedDate": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#date"
    },
    "libraCaseNumber": {
      "type": "string",
      "minLength": 1
    },
    "reason": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false,
  "required": [
    "caseId",
    "reopenedDate",
    "libraCaseNumber",
    "reason"
  ]
}
