{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/event/sjp.events.case-document-added.json",
  "type": "object",
  "properties": {
    "caseId": {
      "id": "/caseId",
      "type": "string"
    },
    "caseDocument": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "materialId": {
          "type": "string"
        },
        "documentType": {
          "type": "string"
        },
        "addedAt": {
          "type": "string"
        }
      },
      "additionalProperties": true,
      "required": [
        "id",
        "materialId",
        "addedAt"
      ]
    },
    "indexWithinDocumentType": {
      "type": "number"
    }
  },
  "additionalProperties": true,
  "required": [
    "caseId",
    "caseDocument",
    "indexWithinDocumentType"
  ]
}
