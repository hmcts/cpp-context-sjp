{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/events/enforcement-pending-application-notification-required.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "applicationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "initiatedTime": {
      "type": "string"
    },
    "gobAccountNumber": {
      "type": "string"
    },
    "divisionCode": {
      "type": "integer"
    },
    "defendantName": {
      "type": "string"
    },
    "urn": {
      "type": "string"
    },
    "dateApplicationIsListed": {
      "type": "string",
      "format": "date"
    },
    "defendantAddress": {
      "type": "string"
    },
    "defendantDateOfBirth": {
      "type": "string"
    },
    "defendantEmail": {
      "type": "string"
    },
    "originalDateOfSentence": {
      "type": "string"
    },
    "defendantContactNumber": {
      "type": "string"
    }
  },
  "required": [
    "caseId",
    "applicationId",
    "initiatedTime",
    "gobAccountNumber",
    "divisionCode",
    "defendantName",
    "urn",
    "dateApplicationIsListed"
  ],
  "additionalProperties": false
}