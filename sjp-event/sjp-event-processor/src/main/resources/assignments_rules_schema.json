{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "assignmentRules": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "courtHouseCodePrefix": {
            "type": "string"
          }
        },
        "required": [
          "courtHouseCodePrefix"
        ],
        "oneOf": [
          {
            "properties": {
              "allowedProsecutingAuthorities": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "allowedProsecutingAuthorities"
            ]
          },
          {
            "properties": {
              "disallowedProsecutingAuthorities": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "disallowedProsecutingAuthorities"
            ]
          }
        ]
      }
    }
  },
  "required": [
    "assignmentRules"
  ]
}
