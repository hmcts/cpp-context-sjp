{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/sjp/prosecution-cases-referred-to-court",
  "type": "object",
  "properties": {
    "prosecutionCaseId": {
      "$ref": "http://cpp.moj.gov.uk/core/domain/json/schema/data-types.json#/definitions/uuid"
    },
    "hearingDays": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/core/courts/hearingDay.json"
      }
    },
    "courtCentre": {
      "$ref": "http://justice.gov.uk/core/courts/courtCentre.json"
    }
  },
  "required": [
    "prosecutionCaseId",
    "hearingDays",
    "courtCentre"
  ]
}