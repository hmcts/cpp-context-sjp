{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "courtReferral": {
      "type": "object",
      "properties": {
        "sjpReferral": {
          "type": "object"
        },
        "prosecutionCases": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              }
            },
            "required": ["id"]
          }
        }
      },
      "required": ["prosecutionCases"]
    },
    "rejectedReason": {
      "type": "string"
    }
  },
  "required": [
    "courtReferral",
    "rejectedReason"
  ],
  "additionalProperties": false
}