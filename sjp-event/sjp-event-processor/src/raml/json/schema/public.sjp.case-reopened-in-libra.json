{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp_public/events/case-reopened-in-libra.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    },
    "reopenedDate": {
      "type": "string",
      "format": "date"
    },
    "libraCaseNumber": {
      "type": "string"
    },
    "reason": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "caseId",
    "reopenedDate",
    "libraCaseNumber"
  ]
}
