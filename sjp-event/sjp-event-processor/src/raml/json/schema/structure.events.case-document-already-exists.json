{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "caseId": {
      "id": "/caseId",
      "type": "string"
    },
    "caseDocument": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "materialId": {
          "type": "string"
        },
        "policeName": {
          "type": "string"
        },
        "policeMaterialId": {
          "type": "string"
        },
        "documentType": {
          "enum": ["SJPN", "CITN", "PLEA", "FINANCIAL_MEANS", "WS", "RESULT_ORDER"]
        },
        "addedAt": {
          "type": "string"
        },
        "externalFileURL": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "materialId",
        "addedAt"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "caseId",
    "caseDocument"
  ]
}
