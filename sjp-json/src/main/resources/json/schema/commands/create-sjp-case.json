{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/commands/sjp.create-sjp-case.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://cpp.moj.gov.uk/core/domain/json/schema/data-types.json#/definitions/uuid"
    },
    "urn": {
      "type": "string"
    },
    "enterpriseId": {
      "type": "string"
    },
    "prosecutingAuthority": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/prosecuting-authority.json"
    },
    "costs": {
      "type": "number"
    },
    "postingDate": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#/definitions/date"
    },
    "defendant": {
      "$ref" : "http://justice.gov.uk/json/schemas/domains/sjp/commands/defendant.json"
    }
  },
  "required": [
    "urn",
    "id",
    "enterpriseId",
    "prosecutingAuthority",
    "postingDate",
    "defendant"
  ]
}
