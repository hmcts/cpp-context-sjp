{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/events/case-assigned.json",
  "type": "object",
  "properties": {
    "caseId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    },
    "assigneeId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    },
    "assignedAt": {
      "description": "Property is not present in old version of event thus it cannot be marked as required",
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#date-time"
    },
    "sessionId": {
      "description": "For the purpose of backward compatibility only",
      "$ref": "http://justice.gov.uk/json/schemas/core/data-types.json#/definitions/uuid"
    }
  },
  "required": [
    "caseId",
    "assigneeId"
  ]
}