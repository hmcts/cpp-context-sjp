{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/events/case-assignment-requested.json",
  "type": "object",
  "properties": {
    "session": {
      "type": "object",
      "properties": {
        "id": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "type": {
          "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/session-type.json"
        },
        "userId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
        },
        "courtHouseCode": {
          "type": "string"
        },
        "localJusticeAreaNationalCourtCode": {
          "description": "backward compatibility",
          "type": "string"
        },
        "prosecutors": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "id",
        "type",
        "userId"
      ]
    }
  },
  "required": [
    "session"
  ],
  "additionalProperties": false
}