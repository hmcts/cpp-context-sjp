{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/queries/offence.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://cpp.moj.gov.uk/core/domain/json/schema/data-types.json#/definitions/uuid"
    },
    "offenceCode": {
      "type": "string"
    },
    "plea": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/plea-type.json"
    },
    "pleaMethod": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/plea-method.json"
    },
    "pleaDate": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#/definitions/date-time"
    },
    "offenceSequenceNumber": {
      "type": "integer"
    },
    "wording": {
      "type": "string"
    },
    "wordingWelsh": {
      "type": "string"
    },
    "cjsCode": {
      "type": "string"
    },
    "sequenceNumber": {
      "type": "integer"
    },
    "startDate": {
      "type": "string"
    },
    "endDate": {
      "type": "string"
    },
    "chargeDate": {
      "type": "string"
    },
    "pendingWithdrawal": {
      "type": "boolean"
    },
    "compensation": {
      "type": "number"
    },
    "prosecutionFacts": {
      "type": "string"
    },
    "backDuty": {
      "type": "number"
    },
    "backDutyDateFrom": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#/definitions/date"
    },
    "backDutyDateTo": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#/definitions/date"
    },
    "vehicleMake": {
      "type": "string",
      "maxLength": 30
    },
    "vehicleRegistrationMark": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/vehicleRegistrationMark"
    },
    "penaltyType": {
      "type": "string"
    },
    "sentencing": {
      "type": "string"
    },
    "backDutyOffence": {
      "type": "boolean"
    }
  },
  "additionalProperties": false
}
