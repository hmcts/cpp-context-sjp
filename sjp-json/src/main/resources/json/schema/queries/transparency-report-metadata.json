{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/queries/transparency-report-metadata.json",
  "type": "object",
  "properties": {
    "reportsMetadata": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "generatedAt": {
            "type": "string"
          },
          "pages": {
            "type": "number"
          },
          "reportIn": {
            "type": "string"
          },
          "size": {
            "type": "string"
          },
          "fileId": {
            "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
          }
        }
      }
    }
  },
  "required": [
    "reportsMetadata"
  ],
  "additionalProperties": false
}