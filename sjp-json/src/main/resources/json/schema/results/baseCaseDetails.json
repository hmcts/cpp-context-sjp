{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/results/baseCaseDetails.json",
  "type": "object",
  "description": "Prosecution Case File Details",
  "properties": {
    "caseId": {
      "$ref": "http://cpp.moj.gov.uk/core/domain/json/schema/data-types.json#/definitions/uuid"
    },
    "urn": {
      "type": "string"
    },
    "defendants": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/caseDefendant.json"
      }
    },
    "prosecutingAuthority": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "caseId",
    "urn",
    "defendants"
  ]
}
