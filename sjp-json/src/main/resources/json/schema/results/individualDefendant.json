{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/results/individualDefendant.json",
  "type": "object",
  "properties": {
    "basePersonDetails": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/basePersonDetail.json"
    },
    "pncIdentifier": {
      "type": "string"
    },
    "personStatedNationality": {
      "type": "string"
    },
    "bailConditions": {
      "type": "string"
    },
    "bailStatus": {
      "type": "string"
    },
    "reasonForBailConditionsOrCustody": {
      "type": "string"
    },
    "presentAtHearing": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "basePersonDetails",
    "bailStatus",
    "presentAtHearing"
  ]
}
