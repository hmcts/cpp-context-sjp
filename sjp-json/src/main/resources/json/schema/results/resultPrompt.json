{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/results/resultPrompt.json",
  "description": "The court clerk",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "resultDefinitionId": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "isAvailableForCourtExtract": {
      "description": "Indicates if the result prompt is available to be included in a court extract",
      "type": "boolean"
    },
    "promptSequence": {
      "type": "number"
    },
    "promptReference": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "welshLabel": {
      "type": "string"
    },
    "value": {
      "type": "string"
    },
    "welshValue": {
      "type": "string"
    },
    "fixedListCode": {
      "type": "string"
    },
    "usergroups": {
      "description": "The user groups that are authorised to view the result prompts",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "isAvailableForCourtExtract",
    "label",
    "value"
  ]
}