{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/results/sharedResultLine.json",
  "description": "The shared results from a hearing resulting process",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "delegatedPowers": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/delegatedPowers.json"
    },
    "isFinancial": {
      "description": "Indicates if the result is a financial result",
      "type": "boolean"
    },
    "isAvailableForCourtExtract": {
      "description": "Indicates if the result is available to be included in a court extract",
      "type": "boolean"
    },
    "prosecutionCaseId": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "defendantId": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "offenceId": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/uuid"
    },
    "level": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "welshLabel": {
      "type": "string"
    },
    "rank": {
      "type": "number"
    },
    "lastSharedDateTime": {
      "type": "string"
    },
    "orderedDate": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/courtsDefinitions.json#/definitions/datePattern"
    },
    "usergroups": {
      "description": "The user groups that are authorised to view the result",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "courtClerk": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/delegatedPowers.json"
    },
    "prompts": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/results/resultPrompt.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "level",
    "label",
    "courtClerk",
    "isAvailableForCourtExtract"
  ]
}