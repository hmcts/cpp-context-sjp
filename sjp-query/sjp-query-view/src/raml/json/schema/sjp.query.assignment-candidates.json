{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "assignmentCandidates": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "caseId": {
            "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
          },
          "caseStreamVersion": {
            "type": "integer",
            "minimum": 1
          }
        },
        "required": [
          "caseId",
          "caseStreamVersion"
        ]
      }
    }
  },
  "required": [
    "assignmentCandidates"
  ]
}
