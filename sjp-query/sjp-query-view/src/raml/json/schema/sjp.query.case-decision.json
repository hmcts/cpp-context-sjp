{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/sjp/queries/case-decision.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "session": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/queries/session.json"
    },
    "savedAt": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/temporal.json#/definitions/date-time"
    },
    "offenceDecisions": {
      "type": "array",
      "items": {
        "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/queries/query-offence-decision.json"
      }
    },
    "financialImposition": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/queries/query-financial-imposition.json"
    },
    "applicationDecision": {
      "$ref": "http://justice.gov.uk/json/schemas/domains/sjp/queries/query-application-decision.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "session",
    "savedAt",
    "offenceDecisions"
  ]
}