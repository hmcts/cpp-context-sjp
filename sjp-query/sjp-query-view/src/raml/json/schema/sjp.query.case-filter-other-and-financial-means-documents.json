{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "",
  "type": "object",
  "properties": {
    "id": {
      "id": "/id",
      "type": "string"
    },
    "enterpriseId": {
      "id": "/enterpriseId",
      "type": "string",
      "minLength": 12,
      "maxLength": 12
    },
    "version": {
      "id": "/version",
      "type": "integer"
    },
    "urn": {
      "id": "/urn",
      "type": "string"
    },
    "dateTimeCreated": {
      "id": "/dateTimeCreated",
      "type": "string"
    },
    "completed": {
      "id": "/completed",
      "type": "boolean"
    },
    "costs": {
      "id": "/costs",
      "type": "number"
    },
    "postingDate": {
      "id": "/postingDate",
      "type": "string"
    },
    "reopenedDate": {
      "id": "/reopenedDate",
      "type": "string"
    },
    "libraCaseNumber": {
      "id": "/libraCaseNumber",
      "type": "string"
    },
    "reopenedInLibraReason": {
      "type": "string"
    },
    "caseDocuments": {
      "id": "/caseDocuments",
      "type": "array",
      "items": {
        "id": "/caseDocuments/0",
        "type": "object",
        "properties": {
          "id": {
            "id": "/caseDocuments/0/id",
            "type": "string"
          },
          "materialId": {
            "id": "/caseDocuments/0/materialId",
            "type": "string"
          }
        }
      }
    },
    "defendants": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "caseId": {
            "type": "string"
          },
          "personId": {
            "type": "string"
          },
          "interpreter": {
            "type": "object",
            "properties": {
              "needed": {
                "type": "boolean"
              },
              "language": {
                "type": "string"
              }
            }
          },
          "numPreviousConvictions": {
            "type": "integer"
          },
          "offences": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "offenceCode": {
                  "type": "string"
                },
                "cjsCode": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "plea": {
                  "type": "string"
                },
                "offenceSequenceNumber": {
                  "type": "integer"
                },
                "pleaMitigation": {
                  "type": "string"
                },
                "wording": {
                  "type": "string"
                },
                "pleaMethod": {
                  "enum": [
                    "ONLINE",
                    "POSTAL"
                  ]
                },
                "description": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "pendingWithdrawal": {
                  "type": "boolean"
                },
                "compensation": {
                  "type": "number"
                },
                "prosecutionFacts": {
                  "type": "string"
                },
                "startDate": {
                  "type": "string"
                },
                "endDate": {
                  "type": "string"
                },
                "chargeDate": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "prosecutingAuthority": {
      "id": "/prosecutingAuthority",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "urn"
  ]
}