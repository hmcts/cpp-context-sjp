{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "caseId": {
      "type": "string"
    },
    "personId": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "urn": {
      "type": "string"
    },
    "initiationCode": {
      "type": "string"
    },
    "prosecutingAuthority": {
      "id": "/prosecutingAuthority",
      "type": "string",
      "enum": [
        "TFL",
        "TVL",
        "DVLA"
      ]
    },
    "postingDate": {
      "type": "string"
    },
    "assigned": {
      "type": "boolean"
    },
    "completed": {
      "type": "boolean"
    },
    "enterpriseId": {
      "type": "string",
      "minLength": 12,
      "maxLength": 12
    },
    "reopendDate": {
      "type": "string"
    },
    "pleaDate": {
      "type": "string"
    },
    "withdrawalRequestedDate": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "string"
    },
    "postCode": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "caseId",
    "urn",
    "enterpriseId",
    "initiationCode",
    "prosecutingAuthority",
    "lastName"
  ]
}
