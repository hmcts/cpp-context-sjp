<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="11" author="gregh" logicalFilePath="liquibase/sjp-view-store-db-changelog.xml">
        <addColumn tableName="case_details">
            <column name="online_plea_received" type="BOOLEAN" defaultValueBoolean="false"></column>
        </addColumn>
        <addColumn tableName="online_plea">
            <column name="plea" type="TEXT"></column>
            <column name="come_to_court" type="TEXT"></column>
            <column name="mitigation" type="TEXT"></column>
            <column name="not_guilty_because" type="TEXT"></column>
        </addColumn>
    </changeSet>

</databaseChangeLog>
