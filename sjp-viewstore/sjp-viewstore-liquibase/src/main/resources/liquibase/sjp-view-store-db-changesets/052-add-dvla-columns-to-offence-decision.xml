<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="52" author="venu.vemula"
               logicalFilePath="liquibase/sjp-view-store-db-changelog.xml">

        <addColumn tableName="offence_decision">
            <column name="back_duty" type="currency"/>
            <column name="excise_penalty" type="currency"/>
        </addColumn>

        <rollback>
            <dropColumn tableName="offence_decision">
                <column name="back_duty" />
            </dropColumn>
            <dropColumn tableName="offence_decision">
                <column name="excise_penalty" />
            </dropColumn>
        </rollback>
    </changeSet>

</databaseChangeLog>
