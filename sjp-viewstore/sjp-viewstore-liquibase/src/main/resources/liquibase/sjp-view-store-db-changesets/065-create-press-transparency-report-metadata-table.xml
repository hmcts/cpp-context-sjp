<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="65" author="team/ATCM/Mojo/gustavo.rojas"
               logicalFilePath="liquibase/create-press-transparency-report-metadata-table.xml">
        <createTable tableName="press_transparency_report_metadata">
            <column name="id" type="UUID">
                <constraints primaryKey="true"/>
            </column>
            <column name="file_service_id" type="uuid"/>
            <column name="number_of_pages" type="integer"/>
            <column name="size_in_bytes" type="integer"/>
            <column name="generated_at" type="datetime"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
