<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="082-add-pcq-visited-to-defendant.xml" author="manav.arora"
               logicalFilePath="liquibase/sjp-view-store-db-changelog.xml">

        <addColumn tableName="defendant">
            <column name="pcq_visited" type="boolean"/>
        </addColumn>

        <rollback>
            <dropColumn tableName="defendant">
                <column name="pcq_visited"/>
            </dropColumn>
        </rollback>

    </changeSet>

</databaseChangeLog>
